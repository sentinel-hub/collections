<!DOCTYPE html>
<html lang="en">
  {{> header isHome=isHome}}

    {{> about}}
    <div class="col-md-6 col-md-offset-1 datasets">
      <h2 class="h2_addition">Collections Additions</h2>
      {{#each datasets}}
      <div class="row">
      <h3>{{@key}}</h3>
      <hr>
      {{#each this}}
        <div id="{{Slug}}" class="dataset">
          <p><a href="{{rootUrl}}{{Slug}}/">{{Name}}</a></p>
        </div>
        {{/each}}
      </div>
      {{/each}}
    </div>


  {{> footer}}
  </body>
  <script src="{{rootUrl}}js/index.js"></script>
</html>

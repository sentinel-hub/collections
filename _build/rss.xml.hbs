<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Sentinel Hub Public Collections</title>
    <link>https://collections.sentinel-hub.com/</link>
    <description>Public collections available within Sentinel Hub</description>
    <language>en-us</language>
    <managingEditor>info@sentinel-hub.com</managingEditor>
    <lastBuildDate>{{buildDate}}</lastBuildDate>
    {{#each datasets}}
    <item>
      <title>{{Name}}</title>
      <link>{{rootUrl}}{{Slug}}</link>
      <guid>{{rootUrl}}{{Slug}}</guid>
      <description>{{md Description}}</description>
    </item>
    {{/each}}
  </channel>
</rss>
